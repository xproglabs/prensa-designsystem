{
  "name": "@xprog/prensa-monorepo",
  "version": "1.0.0",
  "author": "XPROG",
  "repository": "https://github.com/xproglabs/prensa-designsystem.git",
  "scripts": {
    "prensa:v1": "cd packages/prensa && yarn storybook",
    "prensa:v2": "cd packages/xprog-prensa",
    "build:prensa:v1": "cd packages/prensa && yarn build",
    "build:prensa:v2": "cd packages/xprog-prensa && yarn build",
    "release:local:prensa:v1": "cd packages/prensa && yarn release:local",
    "release:local:prensa:v2": "cd packages/xprog-prensa && yarn build && yarn release:local",
    "release:prensa:v1": "cd packages/prensa && yarn semantic-release --branches monorepo",
    "release:prensa:v2": "cd packages/xprog-prensa && yarn semantic-release",
    "deps:prensa:v1": "cd packages/prensa && yarn",
    "deps:prensa:v2": "cd packages/xprog-prensa && yarn"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {}
}
