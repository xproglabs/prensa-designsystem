  "scripts": {
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -o ./storybookbuild",
    "build": "rollup --config",
    "eslint": "eslint -c .eslintrc.json --fix --ext js,ts,tsx ./",
    "run-prod": "npx http-server /var/www/prensa-designsystem/storybookbuild",
    "test": "jest",
    "clear:dist": "rm -rf dist",
    "prerelease:copyfiles": "cp -rf README.md dist && cp -rf CHANGELOG.md dist && cp -rf package.json dist",
    "release:local": "npm run clear:dist && npm run build && npm run prerelease:copyfiles && cd dist && npm pack",
    "release:qa": "npm run clear:dist && npm run build && npm run prerelease:copyfiles && cd dist && npm version patch && npm run eslint && git push --tags && npm publish --tag qa",
