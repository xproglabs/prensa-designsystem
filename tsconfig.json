{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@PrensaEngine": [
        "prensa.local.config.ts"
      ]
    }
  },
  "exclude": ["node_modules", "dist"],
  "include": [ "*" ]
}